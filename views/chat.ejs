		<link class="codestyle" rel="stylesheet" href="/css/dropzone.css">
		<link rel="stylesheet" href="/css/chat.css">
		<link rel="stylesheet" href="/css/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
        <link rel="stylesheet" href="/css/styles/zenburn.css">
        <link class="codestyle" rel="stylesheet" href="/css/styles/default.css" disabled="">
		<link class="codestyle" rel="stylesheet" href="/css/styles/solarized_dark.css" disabled="">
		<link class="codestyle" rel="stylesheet" href="/css/styles/solarized_light.css" disabled="">
		<link class="codestyle" rel="stylesheet" href="/css/styles/github.css">
